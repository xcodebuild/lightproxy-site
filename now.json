{
    "version": 2,
    "alias": [
    ],
    "routes": [
        {
            "src": "/(.*\\.js)",
            "headers": {
                "cache-control": "public, max-age=3600"
            },
            "dest": "https://alibaba.github.io/lightproxy/$1"
        },
        {
            "src": "/(.*\\.css)",
            "headers": {
                "cache-control": "public, max-age=3600"
            },
            "dest": "https://alibaba.github.io/lightproxy/$1"
        },
        {
            "src": "/(.*\\.png)",
            "headers": {
                "cache-control": "public, max-age=3600"
            },
            "dest": "https://alibaba.github.io/lightproxy/$1"
        },
        {
            "src": "/(.*\\.jpeg)",
            "headers": {
                "cache-control": "public, max-age=3600"
            },
            "dest": "https://alibaba.github.io/lightproxy/$1"
        },
        {
            "src": "/(.*)",
            "headers": {
                "cache-control": "public, max-age=3600"
            },
            "dest": "https://alibaba.github.io/lightproxy/"
        }
    ],
    "github": {
        "enabled": true,
        "autoJobCancelation": true
    }
}
