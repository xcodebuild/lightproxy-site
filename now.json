{
    "version": 2,
    "alias": [
    ],
    "routes": [
        {
            "src": "/(.*\\.js|.*\\.css|.*\\.png|.*\\.jpeg)",
            "headers": {
                "cache-control": "public, max-age=3600"
            },
            "dest": "https://alibaba.github.io/lightproxy/$1"
        },
        {
            "src": "/(.*)",
            "headers": {
                "cache-control": "public, max-age=3600"
            },
            "dest": "https://alibaba.github.io/lightproxy/index.html"
        }
    ],
    "github": {
        "enabled": true,
        "autoJobCancelation": true
    }
}
